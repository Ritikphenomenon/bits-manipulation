
#include <iostream>
#include<vector>
using namespace std;
	// your code goes here
    /*
    given 2n+2  unique number
    where every number is coming twice
    except two number find those two unique
    number
    */
    void uniqueno2(vector<int>arr)
    {
        int n=arr.size();
        int result=0;
        for(int i=0;i<arr.size();i++)
        {
            result=result^arr[i];
        }
        //position
        int pos=0;
        int temp=result;
        while((temp&1)==0)
        {
            pos++;
            temp=temp>>1;
        }
        //filter out the nmbers from thye array which have set bit at 'pos
        int seta=0;
        int setb=0;
        int mask=(1<<pos);
        for(int i=0;i<arr.size();i++)
        {
            if((arr[i]&mask)>0)
            {
                seta=seta^arr[i];
            }
            else
            {
                setb=setb^arr[i];
            }
        }
        cout<<seta<<"\n";
        cout<<setb<<"\n";
    }
    int main()
    {
        vector<int>arr={1,3,5,4,3,1,5,7};
        uniqueno2(arr);
    
    	return 0;
}

