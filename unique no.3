    /*
    given 3n+1  unique number
    where every number is coming thrice
    except one number. find that  unique
    number
    */
    void updatesum(vector<int>&sumarr,int x)
    {
        for(int i=0;i<32;i++)
        {
            int ith_bit=x&(1<<i);
            if(ith_bit)
            {
                sumarr[i]++;
            }
        }
    }
    int numfrombits(vector<int>sumarr)
    {
        int num=0;
        for(int i=0;i<32;i++)
        {
            num=num+(sumarr[i]*(1<<i));
        }
        return num;
    }
    int uniqueno3(vector<int>arr)
    {
        vector<int>sumarr(32,0);//fill constructor
        for(int x:arr)
        {
            updatesum(sumarr,x);
        }
        for(int i=0;i<32;i++)
        {
            sumarr[i]=sumarr[i]%3;
        }
        // an array of bits(0 or 1)
        return numfrombits(sumarr);
    }
    int main()
    {
        vector<int>arr={1,3,5,4,3,1,5,5,3,1};
      cout<<  uniqueno3(arr);
    
